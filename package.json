{
  "name": "effectful-urql",
  "version": "0.1.1",
  "description": "Browser package for urql with Effect-TS integration",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./svelte": {
      "types": "./src/index.svelte.ts",
      "import": "./src/index.svelte.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "keywords": [
    "typescript",
    "urql",
    "effect",
    "browser",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@urql/core": ">=6.0.1",
    "effect": ">=3.0.0",
    "svelte": "^5.40.1"
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "effect": "^3.10.0",
    "happy-dom": "^20.0.0",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ]
}
